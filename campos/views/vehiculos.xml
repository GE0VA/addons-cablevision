<odoo>
    <data>
        <record id="vehiculos_form_view" model="ir.ui.view">
        <field name="name">Vista Vehiculos</field>
        <field name="model">vehiculos.oodo</field>
        <field name="arch" type="xml">
            <form string="formulario">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="placa"/>
                        <field name="description"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
        <record id="vehiculo_tree_view" model="ir.ui.view">
            <field name="name">lista vehiculos</field>
            <field name="model">vehiculos.oodo</field>
            <field name="arch" type="xml">
                <tree string="Lista de Vehiculos">
                    <field name="name"/>
                    <field name="placa"/>
                    <field name="description"/>
                </tree>
            </field>
        </record>
        <record id="vehiculo_action_view" model="ir.actions.act_window">
            <field name="name">Vehiculos</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">vehiculos.oodo</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">

              <p class="oe_view_nocontent_create">
                <!-- Add Text Here -->
              </p>
            </field>
        </record>
        <menuitem id="UNIQUE_ID_categ7" name="Vehiculos" parent="UNIQUE_ID_categ3" action="vehiculo_action_view" />
    </data>
</odoo>